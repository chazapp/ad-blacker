{
    "manifest_version": 2,
    "name": "Ad-Blacker",
    "version": "1.0",
    "description": "Blacks-out ads on popular video streaming websites",
  
    "icons": {
      "48": "icons/48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["https://www.twitch.tv/*"],
        "js": ["ad-blacker.js"]
      }
    ]
}
  
