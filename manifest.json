{
    "manifest_version": 2,
    "name": "Ad-Blacker",
    "version": "1.0",
    "description": "Blacks-out adds on popular video streaming websites",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["ad-blacker.js"]
      }
    ]
}
  
